{
    "mode": "local",
    "external": {
        "searchpath": "/usr/local/lib/python"
    },
    "tmp": "/tmp",
    "log": {
        "file": "../conf/log.conf"
    },
    "database": {
        "type": "sqlite",
        "bind": "sqlite:///../db/akiyoshi.db",
        "pool_size": 10,
        "max_overflow": 10
    },
    "collectd": {
        "basedir": "/var/collectd"
    },
    "auth": {
        "type": "basic"
    },
    "admin": {
        "user" : "admin",
        "password" : "admin"
    },
    "plugin": {
        "rrd": ["cpu", "df", "disk", "interface", "load", "memory", "uptime", "users"]
    }
}
